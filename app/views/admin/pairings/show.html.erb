<%- model_class = Pairing -%>
<%- model_class_trans = PairingTranslation -%>
<% title t '.title', :default => model_class.model_name.human %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_pairings_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_pairing_path(@pairing), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_pairing_path(@pairing),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
</div>



<%= render :partial => 'shared/draggable_images' %>

